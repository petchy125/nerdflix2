{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nerdflix-nx-cypress/shared": ["libs/src/index.ts"],
      "@nerdflix-nx-cypress/shared/assets/*": ["libs/shared/assets/*"],
      "@nerdflix-nx-cypress/shared/common/*": ["libs/shared/common/*"],
      "@nerdflix-nx-cypress/shared/components/*": [
        "libs/shared/components/src/lib/*"
      ],
      "@nerdflix-nx-cypress/shared/constants/*": ["libs/shared/constants/*"],
      "@nerdflix-nx-cypress/shared/container/*": [
        "libs/shared/container/src/lib/*"
      ],
      "@nerdflix-nx-cypress/shared/fixtures/*": ["libs/shared/fixtures/*"],
      "@nerdflix-nx-cypress/shared/providers/*": ["libs/shared/providers/*"],
      "@nerdflix-nx-cypress/shared/server": ["libs/src/server.ts"],
      "@nerdflix-nx-cypress/shared/types": ["libs/shared/types/index.ts"],
      "@nerdflix-nx-cypress/shared/utils/*": ["libs/shared/utils/*"],
      "@nerdflix2/shared-component": ["libs/shared/components/src/index.ts"],
      "@nerdflix2/shared-container": ["libs/shared/container/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
